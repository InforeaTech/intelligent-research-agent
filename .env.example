# Environment Variables Configuration
# Copy this file to .env and fill in your actual API keys

# AI Provider API Keys (choose at least one)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GROK_API_KEY=your_grok_api_key_here

# Optional: Search Enhancement
SERPER_API_KEY=your_serper_api_key_here

# JWT Configuration (for authentication)
JWT_SECRET_KEY=your-secret-key-generate-a-strong-random-string
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# OAuth Providers - Google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# OAuth Providers - Microsoft
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret

# OAuth Providers - GitHub
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# OAuth Configuration
OAUTH_REDIRECT_URI=http://localhost:8000/auth/callback
FRONTEND_URL=http://localhost:8000

# Note: You can also store API keys securely using the secrets manager
# by making a POST request to /api/secrets/set endpoint
